{
  "name": "blue-dot-robots-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-seed-data",
    "copy-seed-data": "cp -r src/db-seed-data dist/src/",
    "start": "nodemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check",
    "cloud-seed": "node dist/src/prisma/seed.js",
    "updateCommon": "sudo npm update @bluedotrobots/common-ts"
  },
  "prisma": {
    "seed": "ts-node --files src/prisma/seed.ts"
  },
  "ts-node": {
    "files": true,
    "require": [
      "tsconfig-paths/register"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "."
    ],
    "ext": "ts",
    "exec": "ts-node --files ./src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.0.7",
    "@types/papaparse": "^5.3.15",
    "@types/validator": "^13.12.2",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-plugin-security": "^3.0.1",
    "nodemon": "^3.1.9",
    "prisma": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/client-secrets-manager": "^3.716.0",
    "@bluedotrobots/common-ts": "^1.0.0",
    "@prisma/client": "^6.1.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "google-auth-library": "^10.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^2.0.1",
    "openai": "^5.7.0",
    "papaparse": "^5.5.2",
    "socket.io": "^4.8.1",
    "validator": "^13.12.0",
    "ws": "^8.18.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
