{
  "name": "lever-labs-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && pnpm run copy-seed-data",
    "copy-seed-data": "cp -r src/db-seed-data dist/src/",
    "start": "nodemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit --project tsconfig.test.json",
    "validate": "pnpm run lint && pnpm run type-check && pnpm test",
    "cloud-seed": "node dist/src/prisma/seed.js",
    "updateCommon": "pnpm add @lever-labs/common-ts@latest",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration"
  },
  "ts-node": {
    "files": true,
    "require": [
      "tsconfig-paths/register"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "."
    ],
    "ext": "ts",
    "exec": "ts-node --files ./src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@jest/globals": "^30.1.2",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.13",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.7",
    "@types/papaparse": "^5.3.15",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.12.2",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "jest-environment-node": "^30.1.2",
    "nodemon": "^3.1.9",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.914.0",
    "@aws-sdk/client-secrets-manager": "^3.914.0",
    "@lever-labs/common-ts": "^1.0.325",
    "@prisma/client": "^6.18.0",
    "@typegoose/typegoose": "^12.20.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^4.21.2",
    "gaxios": "^7.1.2",
    "google-auth-library": "^10.4.1",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.19.1",
    "multer": "^2.0.1",
    "node-fetch": "^2.7.0",
    "openai": "^6.6.0",
    "papaparse": "^5.5.2",
    "socket.io": "^4.8.1",
    "validator": "^13.12.0",
    "ws": "^8.18.0"
  }
